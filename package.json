{
  "name": "developers-articles",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "start:devv": "nodemon ./dist/app.js",
    "start:dev": "npx kill-port 3000 && npm run typeorm:migration:run && nodemon src/app.ts",
    "dev2": "ts-node-dev — respawn — pretty — transpile-only app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "init": "tsc --init",
    "tsc": "tsc",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:migration:run": "npm run typeorm migration:run -- -d ./src/config/ormconfig.ts",
    "typeorm:migration:generate": "npm run typeorm -- -d ./src/config/ormconfig.ts migration:generate ./src/migrations/%npm_config_name%",
    "typeorm:migration:create": "npm run typeorm -- migration:create ./src/database/migrations/%npm_config_name%",
    "typeorm:migration:revert": "npm run typeorm -- -d ./src/config/ormconfig.ts migration:revert"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.0",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongo-to-postgres": "^0.0.6",
    "mongoose": "^8.1.1",
    "mongoose-unique-validator": "^5.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.9.9",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "socket.io": "^4.7.4",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "engines": {
    "node": "20.10.0",
    "npm": "10.2.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.12",
    "@types/mongoose-unique-validator": "^1.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/parser": "^6.21.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
